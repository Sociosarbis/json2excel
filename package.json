{
  "name": "json2excel-wasm",
  "description": "WASM based web worker for generating excel files from json",
  "keywords": "excel, convert, wasm",
  "version": "1.0.2",
  "license": "MIT",
  "author": {
    "name": "Aleksei Kolosov"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DHTMLX/json2excel"
  },
  "scripts": {
    "test": "cargo +nightly test -- --nocapture",
    "build-wasm": "cargo build --target wasm32-unknown-unknown --release && wasm-bindgen target/wasm32-unknown-unknown/release/xlsx_import.wasm --target web --out-dir ./wasm",
    "build": "yarn rollup -c"
  },
  "main": "index.js",
  "files": [
    "/index.js",
    "/js",
    "/wasm"
  ],
  "devDependencies": {
    "rollup": "^1.1.2"
  },
  "dependencies": {
    "fast-text-encoding": "^1.0.0"
  }
}
